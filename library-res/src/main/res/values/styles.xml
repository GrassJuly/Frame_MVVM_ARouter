    <!--供应美菜按钮通用组件-->
    <style name="exmine_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@mipmap/count_edittext_bg</item>
        <item name="android:layout_marginTop">@dimen/dp_12</item>
        <item name="android:layout_marginLeft">@dimen/dp_12</item>
        <item name="android:layout_marginRight">@dimen/dp_12</item>
    </style>

    <style name="app_examine_tab">
        <item name="textAllCaps">true</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <!--收入界面et样式-->
    <style name="income_et_type">
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:textColor">@color/color_ff333333</item>
        <item name="android:textColorHint">@color/color_999999</item>
